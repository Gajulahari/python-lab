import math
def jump(arr,x,n):
    step=math.sqrt(n)
    prev=0
    while(arr[int(min(step,n)-1)])<x:
        prev=step
        step+=math.sqrt(n)
        if(prev>=n):
            return-1
    while(arr[int(min(step,n))])<x:
        prev+=1
        if(prev==min(step,n)):
            return-1
    if(arr[int(prev)==x]):
        return prev
    return-1
arr=[1,2,3,4,5,6,7]
x=3
n=len(arr)
index=jump(arr,x,n)
print('num',x,'is index of',"%.0f"%index)
